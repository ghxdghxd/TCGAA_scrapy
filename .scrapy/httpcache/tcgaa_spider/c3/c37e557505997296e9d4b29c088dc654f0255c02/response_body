		<script>
			function $(id){ return document.getElementById(id);
			}
			document.write("<div id=\"ts\" style=\"position:absolute; background-color: white; border: 1px; border-style: solid; border-color: #E5CBCB; color: #676767; font-family: Courier; display: none; z-index: 4;\"></div>");
			function mmove( o, s ) {
				var evt = event||window.event;
				var x = evt.clientX+1;
				var y=evt.clientY+1;
				$('ts').style.left=x+'px';
				$('ts').style.top=y+'px';
				if(s==''||typeof(s)=='undefined'){
					$('ts').innerHTML=o.innerHTML;
				}else{
					$('ts').innerHTML=s;
				}
				$('ts').style.display='';
			}
			function mout(){
				$('ts').style.display='none';
			}
		</script>
		
		<script language="javascript">
			
			function viewpie( graph_title, values, labels, colors ) { 
				values = values.split(",");
				labels = labels.split(",");
				colors = colors.split(",");
				
				var index = colors.indexOf( "WHITE" );
				if (~index) {
					colors[ index ] = "rgb( 0, 255, 0 )";
				}

				var index = colors.indexOf( "BLACK OR AFRICAN AMERICAN" );
				if (~index) {
					colors[ index ] = "rgb( 0, 191, 255 )";
				}

				var index = colors.indexOf( "ASIAN" );
				if (~index) {
					colors[ index ] = "rgb( 221, 160, 221 )";
				}

				var index = colors.indexOf( "AMERICAN INDIAN OR ALASKA NATIVE" );
				if (~index) {
					colors[ index ] = "rgb( 255, 165, 0 )";
				}

				var index = colors.indexOf( "NATIVE HAWAIIAN OR OTHER PACIFIC ISLANDER" );
				if (~index) {
					colors[ index ] = "rgb( 255, 165, 0 )";
				}

				var index = colors.indexOf( "NOT HISPANIC OR LATINO" );
				if (~index) {
					colors[ index ] = "rgb( 0, 0, 0 )";
				}
				
				var index = colors.indexOf( "HISPANIC OR LATINO" );
				if (~index) {
					colors[ index ] = "rgb( 255, 0, 0 )";
				}
				
				var index = colors.indexOf( "European" );
				if (~index) {
					colors[ index ] = "rgb( 173, 255, 47 )";
				}
				
				var index = colors.indexOf( "AfricanAmerican" );
				if (~index) {
					colors[ index ] = "rgb( 0, 191, 255 )";
				}
				
				var index = colors.indexOf( "Asian" );
				if (~index) {
					colors[ index ] = "rgb( 221, 160, 221 )";
				}
				
				var index = colors.indexOf( "NativeAmerican" );
				if (~index) {
					colors[ index ] = "rgb( 255, 165, 0 )";
				}
				
				var index = colors.indexOf( "Other" );
				if (~index) {
					colors[ index ] = "rgb( 190, 190, 190 )";
				}
				
				var index = colors.indexOf( "missing" );
				if (~index) {
					colors[ index ] = "rgb( 190, 190, 190 )";
				}
				
				var index = colors.indexOf( "White" );
				if (~index) {
					colors[ index ] = "rgb( 0, 255, 0 )";
				}

				var index = colors.indexOf( "Black" );
				if (~index) {
					colors[ index ] = "rgb( 0, 191, 255 )";
				}

				var index = colors.indexOf( "Asian" );
				if (~index) {
					colors[ index ] = "rgb( 221, 160, 221 )";
				}

				var index = colors.indexOf( "American Indian/Alaska Native" );
				if (~index) {
					colors[ index ] = "rgb( 255, 165, 0 )";
				}

				var index = colors.indexOf( "Native Hawaiian/Other Pacific Islander" );
				if (~index) {
					colors[ index ] = "rgb( 255, 165, 0 )";
				}

				var index = colors.indexOf( "non-Hispanic/Latino" );
				if (~index) {
					colors[ index ] = "rgb( 0, 0, 0 )";
				}
				
				var index = colors.indexOf( "Hispanic/Latino" );
				if (~index) {
					colors[ index ] = "rgb( 255, 0, 0 )";
				}
				
				var index = colors.indexOf( "EA" );
				if (~index) {
					colors[ index ] = "rgb( 173, 255, 47 )";
				}
				
				var index = colors.indexOf( "AA" );
				if (~index) {
					colors[ index ] = "rgb( 0, 191, 255 )";
				}
				
				var index = colors.indexOf( "EAA" );
				if (~index) {
					colors[ index ] = "rgb( 221, 160, 221 )";
				}
				
				var index = colors.indexOf( "NA" );
				if (~index) {
					colors[ index ] = "rgb( 255, 165, 0 )";
				}
				
				var index = colors.indexOf( "OA" );
				if (~index) {
					colors[ index ] = "rgb( 190, 190, 190 )";
				}
				
				var index = colors.indexOf( "Not Available" );
				if (~index) {
					colors[ index ] = "rgb( 190, 190, 190 )";
				}
				
				var data = [{
					values: values,
					labels: labels,
					textinfo: 'none', 
					type: 'pie', 
					hoverinfo: 'label+value+percent', 
					marker: {
						colors: colors
					}
				}];
				var layout = {
					width: 300, 
					height: 200, 
					margin: {
						l: 50,
						r: 50,
						b: 40,
						t: 40,
						pad: 4
					  },
					autosize: false, 
					showlegend: false
				};
				Plotly.plot( graph_title, data, layout);
			}
			
			function viewBAR( graph_title, num, label ) { 
				label = label.split(",");
				num = num.split(",");
				var myPlot = document.getElementById(graph_title),
					data = [{x:label, y:num, type:'bar'}],
					layout = {barmode: 'stack', scene: {yaxis: {title: 'Cases'}}, 
							width: 300, 
							height: 300, 
							margin: {
								l: 50,
								r: 50,
								b: 40,
								t: 40,
								pad: 4
							  },
							autosize: false, 
							showlegend: false
						 };
				Plotly.plot(graph_title, data, layout);
				dragLayer = document.getElementsByClassName('nsewdrag')[0]
				myPlot.on('plotly_hover', function(data){
					dragLayer.style.cursor = 'pointer'
				});
				myPlot.on('plotly_unhover', function(data){
				  dragLayer.style.cursor = 'default'
				});
				myPlot.on('plotly_click', function(data){
					var pts = '';
					for(var i=0; i<data.points.length; i++){
						cancertype = data.points[i].x;
						newaddress = 'patients.php?cancertype=' + cancertype; 
						window.open(newaddress);
					}
				});
			}
			
		</script>
		
		<script language="javascript">
			function showContent(sections_str,content) {
				sections = sections_str.split(",");
				for(i=0; i<sections.length; i++){
					if(document.getElementById(sections[i]).style.display == "none" && sections[i] == content){
						document.getElementById(sections[i]).style.display = "";
					}else{
						document.getElementById(sections[i]).style.display = "none";
					}
				}
			}
		</script>
		
